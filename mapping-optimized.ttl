@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rr:     <http://www.w3.org/ns/r2rml#> .
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix schema: <http://schema.org/> .
@prefix ex:     <http://example.org/vocab#> .

@base <http://example.org/> .

# TriplesMap pour les communes

<#CommunesMap> a rr:TriplesMap ;
    rml:logicalSource [
        a rml:LogicalSource ;
        rml:source "communes-france-2025.csv" ;
        rml:referenceFormulation ql:CSV ;
    ] ;

    # URI de chaque commune basée sur le code INSEE
    rr:subjectMap [
        rr:template "http://example.org/commune/{code_insee}" ;
        rr:class schema:City ;
    ] ;

    # Nom de la commune
   rr:predicateObjectMap [
        rr:predicate schema:name ;
        rr:objectMap [ rml:reference "nom_standard" ] ;
    ] ;

    # Code INSEE
    rr:predicateObjectMap [
        rr:predicate ex:codeInsee ;
        rr:objectMap [ rml:reference "code_insee" ] ;
    ] ;

    # Code postal
    rr:predicateObjectMap [
        rr:predicate schema:postalCode ;
        rr:objectMap [ rml:reference "code_postal" ] ;
    ] ;

    # Département
    rr:predicateObjectMap [
        rr:predicate ex:departement ;
        rr:objectMap [ rml:reference "dep_nom" ] ;
    ] ;

    # Région
    rr:predicateObjectMap [
        rr:predicate ex:region ;
        rr:objectMap [ rml:reference "reg_nom" ] ;
    ] ;

    # Population de la commune
    rr:predicateObjectMap [
        rr:predicate ex:population ;
        rr:objectMap [
            rml:reference "population" ;
            rr:datatype xsd:integer
        ]
    ] .

# TriplesMap pour les stations (physiques, sans prix)
# On groupe par ID de station pour éviter les doublons

<#StationsMap> a rr:TriplesMap ;
    rml:logicalSource [
        a rml:LogicalSource ;
        rml:source "prix-carburants-quotidien.json" ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$[*]" ;
    ] ;

    # URI de la station basée sur son id
    rr:subjectMap [
        rr:template "http://example.org/station/{id}" ;
        rr:class schema:GasStation ;
    ] ;

    # Identifiant de la station
    rr:predicateObjectMap [
        rr:predicate ex:stationId ;
        rr:objectMap [ rml:reference "id" ] ;
    ] ;

    # Adresse de la station
    rr:predicateObjectMap [
        rr:predicate schema:streetAddress ;
        rr:objectMap [ rml:reference "adresse" ] ;
    ] ;


    # rr:predicateObjectMap [
    #     rr:predicate schema:postalCode ;
    #     rr:objectMap [ rml:reference "cp" ] ;
    # ] ;

    # Coordonnées GPS de la station
    rr:predicateObjectMap [
        rr:predicate schema:latitude ;
        rr:objectMap [
            rml:reference "geom.lat" ;
            rr:datatype xsd:decimal
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:longitude ;
        rr:objectMap [
            rml:reference "geom.lon" ;
            rr:datatype xsd:decimal
        ]
    ] ;

    # Jointure avec la commune via code INSEE
    rr:predicateObjectMap [
        rr:predicate ex:locatedIn ;
        rr:objectMap [
            rr:parentTriplesMap <#CommunesMap> ;
            rr:joinCondition [
                rr:child  "com_arm_code" ;
                rr:parent "code_insee" ; 
            ]
        ]
    ] .

# TriplesMap pour les prix des carburants

<#PricesMap> a rr:TriplesMap ;
    rml:logicalSource [
        a rml:LogicalSource ;
        rml:source "prix-carburants-quotidien.json" ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$[*]" ;
    ] ;

    # URI unique pour chaque offre de prix
    rr:subjectMap [
        rr:template "http://example.org/station/{id}/price/{prix_id}" ;
        rr:class schema:Offer ;
    ] ;

    # Type de carburant
    rr:predicateObjectMap [
        rr:predicate ex:fuelType ;
        rr:objectMap [ rml:reference "prix_nom" ] ;
    ] ;

    # Prix
    rr:predicateObjectMap [
        rr:predicate schema:price ;
        rr:objectMap [
            rml:reference "prix_valeur" ;
            rr:datatype xsd:decimal
        ]
    ] ;

    # Lien vers la station
    rr:predicateObjectMap [
        rr:predicate schema:offeredBy ;
        rr:objectMap [
            rr:template "http://example.org/station/{id}" ;
            rr:termType rr:IRI
        ]
    ] .

# TriplesMap pour les instances d'horaires uniques
# Crée une instance par combinaison unique d'heures d'ouverture/fermeture

<#HorairesInstancesMap> a rr:TriplesMap ;
    rml:logicalSource [
        a rml:LogicalSource ;
        rml:source "prix-carburants-quotidien-cleaned.json" ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$[*].horaires.jour[*]" ;
    ] ;

    # URI unique basée sur l'horaire_id
    rr:subjectMap [
        rr:template "http://example.org/horaire/{horaire_id}" ;
        rr:class ex:Horaire ;
    ] ;

    # Heure d'ouverture (si définie)
    rr:predicateObjectMap [
        rr:predicate ex:ouverture ;
        rr:objectMap [
            rml:reference "horaire.@ouverture" ;
            rr:datatype xsd:time
        ]
    ] ;

    # Heure de fermeture (si définie)
    rr:predicateObjectMap [
        rr:predicate ex:fermeture ;
        rr:objectMap [
            rml:reference "horaire.@fermeture" ;
            rr:datatype xsd:time
        ]
    ] .

# TriplesMap pour les jours fermés et non définis
# Traite spécifiquement les horaires sans heures (Ferme et NonDefini)

<#HorairesFermesMap> a rr:TriplesMap ;
    rml:logicalSource [
        a rml:LogicalSource ;
        rml:source "prix-carburants-quotidien-cleaned.json" ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$[*].horaires.jour[?(@.horaire_id == 'Horaire_Ferme')]" ;
    ] ;

    rr:subjectMap [
        rr:template "http://example.org/horaire/Horaire_Ferme" ;
        rr:class ex:Horaire ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ex:ouvert ;
        rr:objectMap [ 
            rr:constant "non" ;
            rr:datatype xsd:string
        ]
    ] .

# TriplesMap pour lier les stations aux horaires par jour - LUNDI

<#StationHorairesLundiMap> a rr:TriplesMap ;
    rml:logicalSource [
        a rml:LogicalSource ;
        rml:source "prix-carburants-quotidien-cleaned.json" ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$[*].horaires.jour[?(@['@id'] == '1')]" ;
    ] ;

    rr:subjectMap [
        rr:template "http://example.org/station/{station_id}" ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ex:horaireLundi ;
        rr:objectMap [
            rr:template "http://example.org/horaire/{horaire_id}" ;
            rr:termType rr:IRI
        ]
    ] .

